algoritmo "creuza02"
// Função :       converter real em dolar
// Autor :
// Data : 18/11/2021
// Seção de Declarações 
var
   reais, dolar: Real
inicio
// Seção de Comandos 
   Escreva("Quantos reias eu tenho? ")
   Leia(reais)
   
   dolar <- reais / 5.57
   Escreva("Tenho US$", dolar)

fimalgoritmo
